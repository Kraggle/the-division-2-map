{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"The Division Map",
		"version":"1.0.9",
		"minimum-overwolf-version":"0.77.10",
		"author":"Kraggle",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"launcher_icon": "icon.ico",
		"description":"The Division Interactive Map"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow": {
				"file":"Files/index.php",
				"transparent": true,
				"resizable": true,
				"use_os_windowing": true,
				"size": {
					"width":700,
					"height":400
					},
				"min_size": {
					"width":400,
					"height":400
				}
			}
		},
		"game_targeting": {
			"type": "dedicated",
			"game_ids": [10848]
		},
		"launch_events": [{
			"event": "GameLaunch",
			"event_data": {
				"game_ids": [10848]
			},
			"start_minimized": true
		}],
		"hotkeys":{
			"divmaphotkeyoverlay": {
				"title":"In-game Overlay",
				"action-type":"toggle",
				"default":"Ctrl+Shift+m"
			}
		}
	}
}
